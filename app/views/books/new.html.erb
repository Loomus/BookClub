<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :pages %>
  <%= f.text_field :pages %>

  <%= f.label :year %>
  <%= f.number_field :year %>

  <%= f.label :cover_image %>
  <%= f.text_field :cover_image, value: "https://ibf.org/site_assets/img/placeholder-book-cover-default.png" %>

  <%= f.label :authors %>
  <%= f.text_field :authors %>

  <%= f.submit %>
<% end %>
